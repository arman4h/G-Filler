{
  "manifest_version": 3,
  "name": "Google Form Autofill Helper",
  "version": "1.0",
  "description": "Auto-fills common Google Form fields like name, email, phone",
  "permissions": ["activeTab", "scripting", "storage"],
  "content_scripts": [
    {
      "matches": ["https://docs.google.com/forms/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
